  },
  "devDependencies": {
    "@types/express": "^5",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^7.18",
    "@typescript-eslint/parser": "^7.18",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.2.1",
    "morgan": "^1.10.0",
    "prettier": "^3.4.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5"
  },
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.5.0"
