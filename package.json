{
  "name": "wf-emulator",
  "version": "0.1.0",
  "description": "WF Emulator",
  "main": "index.ts",
  "scripts": {
    "start": "node --import ./build/src/pathman.js build/src/index.js",
    "dev": "ts-node-dev --openssl-legacy-provider -r tsconfig-paths/register src/index.ts ",
